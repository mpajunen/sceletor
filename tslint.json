{
  "extends": "tslint:recommended",
  "rules": {
    "arrow-parens": [true, "ban-single-arg-parens"],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "object-literal-sort-keys": [
      true,
      "ignore-case",
      "match-declaration-order"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ],
    "semicolon": [
      true,
      "never"
    ]
  }
}
